# AWS Credentials
# Copy this file to .env and fill in your actual credentials
# Never commit .env to version control!
# 
# To use: cp env.example .env
# Then edit .env with your actual credentials

AWS_ACCESS_KEY_ID=your_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here
AWS_REGION=eu-west-1

# Dataset Configuration
MAIN_PATH=/path/to/your/dataset/RadcureComplete/
AWS_BUCKET_NAME=your-bucket-name
AWS_FOLDER=RADCURE/all_cases/

# Optional: Custom paths
ORGAN_DICTIONARY_PATH=/path/to/radcure_dictionary.json
PROCESSED_CASES_FILE=/path/to/processed_cases.txt
FAILED_CASES_FILE=/path/to/processed_cases_failed.txt

